public class ejercicio2 {

	public static void main(String[] args) {
		
		for(int i = 50; i <= 100; i++) {
			
			boolean primo = true;	
			System.out.print(i + ": ");
			
			for(int j = 2; j < i; j++) {
			
				if (i % j == 0) {
					primo = false;
					System.out.print(j + " ");
					break;
				}	
				
			}
			
			if (primo) {
				
				System.out.println("primo");
			}
			else {
				System.out.println(i);
			}	

		}

	}
	
}

